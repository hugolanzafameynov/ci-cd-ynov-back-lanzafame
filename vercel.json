{
  "version": 2,
  "functions": {
    "server.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  }
}
